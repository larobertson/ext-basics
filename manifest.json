{
  "name": "obj ext",
  "description": "my ext",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "16": "./lrclogo-bw.png",
    "32": "./lrclogo-bw.png",
    "48": "./lrclogo-bw.png",
    "128": "./lrclogo-bw.png"
  },
  "background": {
    "scripts": ["./background.js", "./foreground.js"]
  },
  "options_page": "./options.html",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "https://www.google.com/*",
    "storage"
  ]
}